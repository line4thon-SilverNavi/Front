{
  "name": "core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs",
      "default": "./dist/*/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/**/index.ts --dts --format esm,cjs --out-dir dist --clean --treeshake --sourcemap",
    "dev": "tsup src/index.ts src/**/index.ts --dts --format esm,cjs --out-dir dist --watch"
  },
  "peerDependencies": {
    "axios": "^1.7.7",
    "react": ">=18",
    "react-dom": ">=18",
    "styled-components": "^6"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
